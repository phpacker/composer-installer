{
    "name": "phpacker/composer-installer",
    "description": "Install hooks to distribute your PHPacker cross-platform executables through composer",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "gwleuverink",
            "email": "willem@leuver.ink"
        }
    ],
    "autoload": {
        "psr-4": {
            "PHPacker\\ComposerInstaller\\": "src/"
        }
    },
    "require": {
        "composer-runtime-api": "^2.2.2",
        "composer-plugin-api": "^2.2.2",
        "symfony/finder": "^7.2"
    },
    "require-dev": {
        "composer/composer": "^2.2.2",
        "laravel/pint": "^1.21"
    },
    "extra": {
        "bin-dir": "null",
        "class": "PHPacker\\ComposerInstaller\\InstallerPlugin"
    },
    "minimum-stability": "stable"
}
